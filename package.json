{
  "name": "notify-users",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=production && node prod-server/index.js",
    "serve": "vue-cli-service serve",
    "build": "export NODE_ENV=production && babel server --out-dir prod-server && vue-cli-service build",
    "dev": "concurrently \"export NODE_ENV=development\"  \"babel dev-server --out-dir prod-server --watch\" \"nodemon prod-server/index.js\" ",
    "lint": "vue-cli-service lint",
    "start-server": "nodemon ./server/index.js --exec babel-node -e js",
    "create-dev-tables": "babel-node ./server/db/dbConnection createAllTables",
    "initialize-admin-tables": "babel-node ./server/db/dbConnection insertAdminCredentials",
    "drop-dev-tables": "babel-node ./server/db/dbConnection dropAllTables",
    "setup": "npm-run-all -p export-prod start-server create-dev-tables",
    "initialize-db": "npm-run-all -p export-prod initialize-admin-tables",
    "export-prod": "export NODE_ENV=production",
    "reset-db": "npm-run-all -p export-prod  start-server drop-dev-tables"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.8",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "pg": "^8.4.1",
    "twilio": "^3.50.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
